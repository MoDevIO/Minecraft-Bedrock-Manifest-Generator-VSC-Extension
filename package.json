{
  "name": "minecraft-bedrock-manifest-generator",
  "displayName": "Minecraft Bedrock Manifest Generator",
  "description": "A Visual Studio Code extension that helps you generate manifest files for Minecraft Bedrock Edition texture and behavior packs.",
  "version": "0.0.1",
  "publisher": "MoDevIO",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MoDevIO/Minecraft-Bedrock-Manifest-Generator-VSC-Extension"
  },
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "minecraft-bedrock-manifest-generator.bpmanifest",
        "title": "Write Behavior Pack Manifest File"
      },
      {
        "command": "minecraft-bedrock-manifest-generator.tpmanifest",
        "title": "Write Texture Pack Manifest File"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.98.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.21.0"
  },
  "dependencies": {
    "uuid": "^11.1.0"
  }
}
